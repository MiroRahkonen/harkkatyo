<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <TextView
        android:id="@+id/textView_VehicleData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="12dp"
        android:text="(not set)"
        app:layout_constraintStart_toEndOf="@+id/button_MainGotoVehicle"
        app:layout_constraintTop_toTopOf="@+id/button_MainGotoVehicle" />

    <TextView
        android:id="@+id/textView_ConsumptionData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="12dp"
        android:text="(not set)"
        app:layout_constraintStart_toEndOf="@+id/button_MainGotoConsumption"
        app:layout_constraintTop_toTopOf="@+id/button_MainGotoConsumption" />

    <TextView
        android:id="@+id/textView_HousingData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="12dp"
        android:text="(not set)"
        app:layout_constraintStart_toEndOf="@+id/button_MainGotoHousing"
        app:layout_constraintTop_toTopOf="@+id/button_MainGotoHousing" />

    <Button
        android:id="@+id/button_MainGotoConsumption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="124dp"
        android:onClick="gotoConsumption"
        android:text="Go to consumption"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_MainGotoHousing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="96dp"
        android:onClick="gotoHousing"
        android:text="Go to housing"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_MainGotoConsumption" />

    <Button
        android:id="@+id/button_MainGotoVehicle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginTop="100dp"
        android:onClick="gotoVehicle"
        android:text="Go to vehicle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_MainGotoHousing" />

    <Button
        android:id="@+id/button_MainSummary"
        android:layout_width="233dp"
        android:layout_height="69dp"
        android:layout_marginBottom="16dp"
        android:onClick="createSummary"
        android:text="Create summary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>